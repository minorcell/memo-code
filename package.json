{
    "name": "memo-cli",
    "type": "module",
    "private": true,
    "scripts": {
        "start": "bun run packages/ui/src/index.ts",
        "build": "bun build packages/ui/src/index.ts --target bun --outdir dist --minify",
        "format": "bunx prettier --write .",
        "format:check": "bunx prettier --check ."
    },
    "workspaces": [
        "packages/*"
    ],
    "imports": {
        "@memo/core": "./packages/core/src/index.ts",
        "@memo/tools": "./packages/tools/src/index.ts"
    },
    "devDependencies": {
        "@types/bun": "latest",
        "prettier": "^3.3.3"
    },
    "peerDependencies": {
        "typescript": "^5"
    },
    "dependencies": {
        "@dqbd/tiktoken": "^1.0.22",
        "openai": "^6.10.0",
        "toml": "^3.0.0"
    }
}
